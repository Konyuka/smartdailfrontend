 props: {\n  },\n  data(){\n    return {\n      selectedPauseCode:'',\n    }\n  },\n  methods:{\n    close(){\n      this.$parent.sideP = false\n    },\n    submitPause(){\n      this.$store.dispatch(\"setPausecode\", String(this.selectedPauseCode));\n      let payload = { \"username\":localStorage.getItem('user'),\"phone\": localStorage.getItem('phone'),\"campaign\": this.$store.state.campaign,\"state\": 'PAUSED', \"pause_code\" :this.selectedPauseCode};\n      //console.log(payload)\n      return this.$http\n        .post(\"/api/v1/dial/status\",payload, { headers: { \"Content-Type\": \"application/json\", \"Accept\": \"application/json\",\"Authorization\": `Bearer ${localStorage.getItem('token')}`} })\n        .then((response) => {\n            if(response){\n              this.$store.dispatch('userState', 'PAUSED')\n              this.$parent.sideP = false\n              //this.$parent.switchTab('activity')\n              this.$emit('close')\n            }\n          })\n        .catch(error => {\n            console.log(error)\n          })\n    },\n  },\n  computed: {\n    options(){\n      return this.$store.state.codeOptions\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n\n</style>\n"]}]}