r\n  <div id=\"auth\" class=\"h-screen bg-white\">\r\n      \r\n    <div class=\"min-h-screen bg-gray-50 flex flex-col justify-center py-12 sm:px-6 lg:px-8\">\r\n      <div class=\"sm:mx-auto sm:w-full sm:max-w-md\">\r\n        <img class=\"mx-auto h-12 w-auto\" src=\"https://tailwindui.com/img/logos/workflow-mark-indigo-600.svg\" alt=\"Workflow\">\r\n        <h2 class=\"mt-6 text-center text-3xl font-extrabold text-gray-900\">\r\n          Smart Dial Login\r\n        </h2>\r\n      </div>\r\n\r\n      <div class=\"mt-8 sm:mx-auto sm:w-full sm:max-w-md\">\r\n        <div class=\"bg-white py-8 px-4 shadow sm:rounded-lg sm:px-10\">\r\n          <form class=\"space-y-6\" action=\"#\" method=\"POST\">\r\n            <div>\r\n              <label for=\"email\" class=\"block text-sm font-medium text-gray-700\">\r\n                User Name\r\n              </label>\r\n              <div class=\"mt-1\">\r\n                <input v-model=\"username\" id=\"username\" name=\"username\" type=\"text\" autocomplete=\"username\" required class=\"appearance-none block w-full px-3 py-2 border border-gray-300 rounded-md shadow-sm placeholder-gray-400 focus:outline-none focus:ring-indigo-500 focus:border-indigo-500 sm:text-sm\">\r\n              </div>\r\n            </div>\r\n\r\n            <div>\r\n              <label for=\"password\" class=\"block text-sm font-medium text-gray-700\">\r\n                Password\r\n              </label>\r\n              <div class=\"mt-1\">\r\n                <input v-model=\"password\" id=\"password\" name=\"password\" type=\"password\" autocomplete=\"current-password\" required class=\"appearance-none block w-full px-3 py-2 border border-gray-300 rounded-md shadow-sm placeholder-gray-400 focus:outline-none focus:ring-indigo-500 focus:border-indigo-500 sm:text-sm\">\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"flex items-center justify-between\">\r\n              <div class=\"flex items-center\">\r\n                <input id=\"remember_me\" name=\"remember_me\" type=\"checkbox\" class=\"h-4 w-4 text-indigo-600 focus:ring-indigo-500 border-gray-300 rounded\">\r\n                <label for=\"remember_me\" class=\"ml-2 block text-sm text-gray-900\">\r\n                  Remember me\r\n                </label>\r\n              </div>\r\n\r\n              <div class=\"text-sm\">\r\n                <a href=\"#\" class=\"font-medium text-indigo-600 hover:text-indigo-500\">\r\n                  Forgot your password?\r\n                </a>\r\n              </div>\r\n            </div>\r\n\r\n            <div>\r\n              <button @click.prevent=\"login\" type=\"submit\" class=\"w-full flex justify-center py-2 px-4 border border-transparent rounded-md shadow-sm text-sm font-medium text-white bg-indigo-600 hover:bg-indigo-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500\">\r\n                Sign in\r\n              </button>\r\n            </div>\r\n          </form>\r\n\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\n\r\nexport default {\r\n  name: \"Auth\",  \r\n  data(){\r\n    return{\r\n      username: null,\r\n      password: null,\r\n      error: false,\r\n      errors:[],\r\n    }\r\n  },\r\n  methods:{  \r\n      login(){\r\n        let payload = {\r\n            username: this.username, \r\n            password: this.password \r\n        }\r\n        this.error = false\r\n        this.errors = []\r\n        console.log(payload)\r\n        return this.$http.post(\"/api/v1/login\", payload)\r\n        .then(response => {\r\n            this.$store.dispatch('login', response.data)\r\n            localStorage.setItem('token', response.data.token)\r\n            console.log(response)\r\n            this.$router.push('/dialer');\r\n            })\r\n        .catch(error => {\r\n            console.log(error)\r\n            //this.error = true;\r\n            //this.errors = error.response.data.error\r\n          })    \r\n      }\r\n  }     \r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/views"}]}